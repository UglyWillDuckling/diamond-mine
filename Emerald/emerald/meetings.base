filters:
  and:
    - type == "meeting"
    - file.folder != "Sets/Types"
formulas:
  is today: if(scheduled.date() == today(), 'ğŸš©', 'ğŸ“…')
  "has passed ": if(scheduled < now(), 'âœ…', 'ğŸ•§')
  my folder: ""
  schedule_date: scheduled.date() == today()
properties:
  "formula.has passed ":
    displayName: passed
views:
  - type: table
    name: Table
    order:
      - file.name
      - scheduled
      - formula.is today
      - "formula.has passed "
    sort:
      - column: note.scheduled
        direction: DESC
      - column: formula.is today
        direction: DESC
      - column: formula.schedule_date
        direction: ASC
      - column: "formula.has passed "
        direction: ASC
      - column: formula.my folder
        direction: DESC
    columnSize:
      file.name: 315
      note.scheduled: 241
      formula.is today: 136
      "formula.has passed ": 107
  - type: table
    name: 1 on 1's
    filters:
      and:
        - containsAny(property.tags, "meet/1on1/Yoan")
    order:
      - file.name
      - scheduled
      - created
      - duration
      - tags
      - agenda
      - meeting type
      - file.path
      - previous
      - status
    columnSize:
      file.name: 139
      property.scheduled: 241
      property.created: 142
      property.agenda: 151
      file.path: 251
    sort:
      - column: property.scheduled
        direction: DESC
      - column: file.name
        direction: ASC
      - column: property.created
        direction: ASC
  - type: table
    name: today
    filters:
      and:
        - formula["is today"] == "ğŸš©"
    order:
      - scheduled
      - file.name
      - participants
      - "formula.has passed "
    columnSize:
      file.name: 166
      note.scheduled: 192
      note.participants: 237
      "formula.has passed ": 107
    sort:
      - column: formula.is today
        direction: DESC
      - column: property.URL
        direction: ASC
      - column: property.related
        direction: ASC
