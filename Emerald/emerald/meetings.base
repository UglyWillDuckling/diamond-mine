filters:
  and:
    - type == "meeting"
    - file.folder != "Sets/Types"
    - file.folder != "Atlas/Knowledge/templates"
formulas:
  is today: if(scheduled.date() == today(), 'ğŸš©', 'ğŸ“…')
  "has passed ": if(scheduled < now(), 'âœ…', 'ğŸ•§')
  my folder: ""
  schedule_date: scheduled.date() == today()
properties:
  "formula.has passed ":
    displayName: passed
views:
  - type: table
    name: Table
    order:
      - scheduled
      - file.name
      - formula.is today
      - "formula.has passed "
    sort:
      - property: scheduled
        direction: DESC
      - property: formula.is today
        direction: DESC
      - property: formula.schedule_date
        direction: ASC
      - property: "formula.has passed "
        direction: ASC
      - property: formula.my folder
        direction: DESC
    columnSize:
      file.name: 332
      note.scheduled: 241
      formula.is today: 136
      "formula.has passed ": 107
  - type: table
    name: today
    filters:
      and:
        - formula["is today"] == "ğŸš©"
    order:
      - scheduled
      - file.name
      - "formula.has passed "
    sort:
      - property: participants
        direction: ASC
      - property: scheduled
        direction: ASC
      - property: property.related
        direction: ASC
    columnSize:
      note.scheduled: 247
      file.name: 303
      "formula.has passed ": 310
  - type: cards
    name: 1 on 1s
    filters:
      and:
        - tags.contains("meet/1on1/Yoan")
    order:
      - file.name
      - scheduled
    sort:
      - property: scheduled
        direction: DESC
    imageAspectRatio: 1.15
