filters:
  and:
    - type == "meeting"
    - file.folder != "Sets/Types"
    - file.folder != "Atlas/Knowledge/templates"
formulas:
  is today: if(scheduled.date() == today(), 'ðŸš©', 'ðŸ“…')
  "has passed ": if(scheduled < now(), 'âœ…', 'ðŸ•§')
  my folder: ""
  schedule_date: scheduled.date() == today()
  relative: scheduled.relative()
properties:
  "formula.has passed ":
    displayName: passed
views:
  - type: table
    name: Table
    order:
      - scheduled
      - file.name
      - formula.is today
      - "formula.has passed "
    sort:
      - property: scheduled
        direction: ASC
      - property: formula.is today
        direction: DESC
      - property: formula.schedule_date
        direction: ASC
      - property: "formula.has passed "
        direction: ASC
      - property: formula.my folder
        direction: DESC
    columnSize:
      note.scheduled: 282
      file.name: 290
      formula.is today: 136
      "formula.has passed ": 107
  - type: table
    name: today
    filters:
      not:
        - scheduled.date() < today()
        - scheduled.date() > today()
    order:
      - "formula.has passed "
      - file.name
      - formula.relative
    sort:
      - property: formula.relative
        direction: ASC
      - property: scheduled
        direction: DESC
      - property: "formula.has passed "
        direction: ASC
      - property: participants
        direction: ASC
      - property: property.related
        direction: ASC
    columnSize:
      "formula.has passed ": 104
      file.name: 232
      formula.relative: 145
      note.scheduled: 247
  - type: cards
    name: 1 on 1s
    filters:
      and:
        - tags.contains("meet/1on1/Yoan")
    order:
      - file.name
      - scheduled
    sort:
      - property: scheduled
        direction: DESC
    imageAspectRatio: 1.15
